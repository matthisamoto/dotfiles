[alias]
    log-detail     = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
    short          = status -s
    stage          = add
    unstage        = reset HEAD
    staged         = diff --cached
    unstaged       = diff
    stat           = diff --stat -r
    undo           = reset --soft HEAD^
    track          = checkout -t
    prep           = rebase -i @{upstream}
    up             = !sh -c 'git pull --rebase --prune && git log --pretty=format:\"%Cred%ae %Creset- %C(yellow)%s %Creset(%ar)\" HEAD@{1}..'

    human          = name-rev --name-only
    ours           = '!f() { git checkout --ours $@ && git add $@; }; f'
    theirs         = '!f() { git checkout --theirs $@ && git add $@; }; f'
    conflicts      = "!f() { git ls-files -u | awk '{print $4}' | sort -u; }; f"
    edit-conflicts = "!f() { git diff --name-status --diff-filter=U | cut -f2 ; }; $EDITOR `f`"
    prune-remote   = !git remote | xargs -n 1 git remote prune
    new-commits    = !sh -c 'git log $1@{1}..$1@{0} "$@"'
[branch]
    autosetuprebase = always
[color]
    branch = auto
    diff   = auto
    status = auto
    ui     = true
[core]
    excludesfile = /Users/brett/.gitignore
    editor       = vim
[credential]
    helper = osxkeychain
[diff]
    algorithm = patience
[github]
    user = brettbuddin
[help]
    autocorrect = 1
[heroku]
	account = last
[mergetool]
    keepBackup = true
[push]
    default = current
[user]
    name  = Brett Buddin
    email = brett@buddin.us
